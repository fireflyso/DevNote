server {
    listen      443 ssl http2;
    server_name  alist.liuxulu.top;
    client_max_body_size    10000m;

    ssl_certificate_key  /etc/nginx/conf.d/cert/key.pem; # key文件的路径
	ssl_certificate     /etc/nginx/conf.d/cert/cert.pem;  # pem文件的路径
	ssl_session_timeout 5m;
	ssl_ciphers ECDHE-RSA-AES128-GCM-SHA256:ECDHE:ECDH:AES:HIGH:!NULL:!aNULL:!MD5:!ADH:!RC4;
	ssl_protocols TLSv1 TLSv1.1 TLSv1.2;
	ssl_prefer_server_ciphers on;

    location / {
        proxy_pass http://10.0.0.1:5244;
        tcp_nodelay     on;
        proxy_set_header Host            $host;
        proxy_set_header X-Real-IP       $remote_addr;
        proxy_set_header X-Forwarded-For $proxy_add_x_forwarded_for;
        #以上三行，目的是将代理服务器收到的用户的信息传到真实服务器上
    }
    error_log /var/log/error.log debug;
}

server {
    listen      443 ssl http2;
    server_name  bit.liuxulu.top;

    ssl_certificate_key  /etc/nginx/conf.d/cert/bit.key.pem; # key文件的路径
	ssl_certificate     /etc/nginx/conf.d/cert/bit.cert.pem;  # pem文件的路径
	ssl_session_timeout 5m;
	ssl_ciphers ECDHE-RSA-AES128-GCM-SHA256:ECDHE:ECDH:AES:HIGH:!NULL:!aNULL:!MD5:!ADH:!RC4;
	ssl_protocols TLSv1 TLSv1.1 TLSv1.2;
	ssl_prefer_server_ciphers on;

    location / {
        proxy_pass http://10.0.0.1:8888;
        tcp_nodelay     on;
        proxy_set_header Host            $host;
        proxy_set_header X-Real-IP       $remote_addr;
        proxy_set_header X-Forwarded-For $proxy_add_x_forwarded_for;
        #以上三行，目的是将代理服务器收到的用户的信息传到真实服务器上
    }
    error_log /var/log/error.log debug;
}

server {
    listen      443 ssl http2;
    server_name  lan.liuxulu.top;

    ssl_certificate_key  /etc/nginx/conf.d/cert/lan.key.pem; # key文件的路径
	ssl_certificate     /etc/nginx/conf.d/cert/lan.cert.pem;  # pem文件的路径
	ssl_session_timeout 5m;
	ssl_ciphers ECDHE-RSA-AES128-GCM-SHA256:ECDHE:ECDH:AES:HIGH:!NULL:!aNULL:!MD5:!ADH:!RC4;
	ssl_protocols TLSv1 TLSv1.1 TLSv1.2;
	ssl_prefer_server_ciphers on;

    location / {
        proxy_pass http://localhost:3443;
        tcp_nodelay     on;
        proxy_set_header Host            $host;
        proxy_set_header X-Real-IP       $remote_addr;
        proxy_set_header X-Forwarded-For $proxy_add_x_forwarded_for;
        #以上三行，目的是将代理服务器收到的用户的信息传到真实服务器上
    }
    error_log /var/log/error.log debug;
}

server {
    listen      443 ssl http2;
    server_name  img.liuxulu.top;

    ssl_certificate_key  /etc/nginx/conf.d/cert/img.key.pem; # key文件的路径
	ssl_certificate     /etc/nginx/conf.d/cert/img.cert.pem;  # pem文件的路径
	ssl_session_timeout 5m;
	ssl_ciphers ECDHE-RSA-AES128-GCM-SHA256:ECDHE:ECDH:AES:HIGH:!NULL:!aNULL:!MD5:!ADH:!RC4;
	ssl_protocols TLSv1 TLSv1.1 TLSv1.2;
	ssl_prefer_server_ciphers on;

    location / {
        proxy_pass http://10.0.0.2:2283;
        tcp_nodelay     on;
        proxy_set_header Host            $host;
        proxy_set_header X-Real-IP       $remote_addr;
        proxy_set_header X-Forwarded-For $proxy_add_x_forwarded_for;
        #以上三行，目的是将代理服务器收到的用户的信息传到真实服务器上
    }
    error_log /var/log/error.log debug;
}

server {
    listen 80;
    listen [::]:80;
    server_name *.liuxulu.top;
    return 301 https://$http_host$request_uri;
}